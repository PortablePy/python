        -in $top -x 100 -y 140 -width 64 -relwidth 0 -height 33 -relheight 0 \
        -anchor nw -bordermode ignore 
    place $top.but46 \
        -in $top -x 200 -y 210 -width 104 -relwidth 0 -height 33 -relheight 0 \
        -anchor nw -bordermode ignore 
    place $top.mes43 \
        -in $top -x 80 -y 40 -width 350 -relwidth 0 -height 73 -relheight 0 \
        -anchor nw -bordermode ignore 
    } ;# end vTcl:withBusyCursor 

