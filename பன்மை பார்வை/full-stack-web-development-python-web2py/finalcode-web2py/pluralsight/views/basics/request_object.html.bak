{{extend 'layout.html'}}
<h1>Request Object</h1>
<form method='post'>
    <input type='text' name='first_name'>
    <input type='submit' value='Enter'>
</form>
<ul>
    <li>ARGS:  {{=args}}</li>
    <li>VARIABLES:  {{=variables}}</li>
    <li>APPLICATION:  {{=app}}</li>
    <li>CONTROLLER:  {{=cntr}}</li>
    <li>FUNCTION:  {{=fx}}</li>
    <li>EXTENSION:  {{=ext}}</li>
    <li>FOLDER:  {{=folder}}</li>
    <li>NOW:  {{=now}}</li>
    <li>CLIENT:  {{=client}}</li>
    <li>IS SECURE:  {{=isSecure}}</li>
</ul>
